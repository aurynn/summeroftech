<html>
	<body>
		<form action="/done" method="post">
		<ul>
			{% for item in todo %}
				<li><input type="checkbox" id="{{item.id}}">
					{% if item.done %}<del>{%endif%}
					{{ item.content }}
					{%if item.done %}</del>{%endif%}
			</input></li>
			{% endfor %}
		</ul>
		<input type="submit" text="done">
		</form>
	</body>
</html>